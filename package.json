    "dayjs": "^1.11.10",
    "drizzle-orm": "^0.29.3",
    "drizzle-typebox": "^0.1.1",
    "elysia": "latest",
    "mercadopago": "^2.0.6",
    "openai": "^4.26.0",
    "postgres": "^3.4.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@rocketseat/eslint-config": "^2.1.0",
    "@types/pg": "^8.11.0",
    "@types/react": "^18.2.48",
    "bun-types": "^1.0.25",
    "chalk": "^5.3.0",
    "drizzle-kit": "^0.20.13",
